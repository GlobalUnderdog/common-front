{
  "sourceRoot": "src",
  "compact": true,
  "sourceFileName": "src/index.tsx",
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@emotion",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true,
        "corejs": 3
      }
    ]
  ]
}
