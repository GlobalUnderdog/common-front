{
  "name": "@globalunderdog/common-front",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "rm -Rf dist && rm -Rf esm",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "NODE_ENV=production babel src --out-dir esm --extensions '.ts,.tsx'",
    "build:cjs": "NODE_ENV=production babel --config-file ./babel.config.cjs.json src --out-dir dist --extensions '.ts,.tsx'",
    "postbuild": "tsc --noEmit false --emitDeclarationOnly --declaration --declarationDir dist"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@globalunderdog/common": "https://github.com/GlobalUnderdog/common#1.4.1",
