{
  "name": "@globalunderdog/common-front",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "esm",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build-cjs": "rimraf dist && rimraf pre && tsc && rollup -c && cp src/inspectlet.js dist/inspectlet.js && rimraf pre",
    "build-esm": "rimraf esm && tsc --outDir esm --module esnext && cp src/inspectlet.js esm/inspectlet.js",
    "build": "yarn build-cjs && yarn build-esm",
    "push": "yarn build && git push",
    "pushWithTags": "yarn push && git push -u origin --tags"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/css": "^10.0.9",
